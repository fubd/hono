{
  "scripts": {
    "dev:hono": "tsx watch src/index.ts",
    "dev:client": "npm run clean && rspack build --mode development --watch",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:hono\"",
    "build": "npm run clean && rspack build && tsc && cp -r src/sql dist/sql",
    "start": "node dist/index.js",
    "clean": "rimraf ./static/*"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.2",
    "dotenv": "^17.2.0",
    "hono": "^4.7.10",
    "ioredis": "^5.6.1",
    "kysely": "^0.28.2",
    "mysql2": "^3.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tinypool": "^1.1.1"
  },
  "devDependencies": {
    "@rspack/cli": "^1.4.5",
    "@rspack/core": "^1.4.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "22.14.0"
  },
  "type": "module"
}