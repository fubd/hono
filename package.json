{
  "scripts": {
    "dev:hono": "tsx watch src/index.ts",
    "dev:client": "rsbuild build --watch",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:hono\"",
    "build:sql": "cpx \"src/sql/**/*.sql\" dist/src/sql",
    "build": "rsbuild build && tsc && npm run build:sql",
    "start": "node dist/src/index.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.2",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.0",
    "hono": "^4.7.10",
    "inversify": "^7.6.1",
    "ioredis": "^5.6.1",
    "kysely": "^0.28.2",
    "mysql2": "^3.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.8.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tinypool": "^1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@rsbuild/core": "^1.4.13",
    "@rsbuild/plugin-react": "^1.3.4",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.2",
    "cpx": "^1.5.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "22.14.0"
  },
  "type": "module"
}
